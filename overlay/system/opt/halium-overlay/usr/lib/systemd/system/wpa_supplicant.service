[Unit]
Description=WPA supplicant
Before=network.target
After=dbus.service
Wants=network.target
IgnoreOnIsolate=true

[Service]
Type=dbus
BusName=fi.w1.wpa_supplicant1
ExecStart=/usr/bin/bash -c 'cat /vendor/etc/wifi/wpa_supplicant*.conf > /run/vendor_wpa_supplicant.conf; wpa_supplicant -u -s -O /run/wpa_supplicant -c /run/vendor_wpa_supplicant.conf'

[Install]
WantedBy=multi-user.target
Alias=dbus-fi.w1.wpa_supplicant1.service
